rule "First contact"
    salience 100
    when
        initialize: Initialize() from entry-point "agent-created"
        agent: Ava()
    then
        ArrayList<Integer> freeDays = new ArrayList<>(); //Rule Compilation error ArrayList cannot be resolved to a type
        freeDays.add(0);
        agent.sendData(freeDays);
end

